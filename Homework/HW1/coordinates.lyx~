\begin_body

\begin_layout Standard
Rotation Matrices:
\begin_inset Formula 
\begin{equation}
\mathbf{R}_{1}\left[\theta\right]=\begin{bmatrix}1 & 0 & 0\\
0 & \cos\theta & \sin\theta\\
0 & -\sin\theta & \cos\theta
\end{bmatrix},\,\mathbf{R}_{2}\left[\theta\right]=\begin{bmatrix}\cos\theta & 0 & -\sin\theta\\
0 & 1 & 0\\
\sin\theta & 0 & \cos\theta
\end{bmatrix},\,\mathbf{R}_{3}\left[\theta\right]=\begin{bmatrix}\cos\theta & \sin\theta & 0\\
-\sin\theta & \cos\theta & 0\\
0 & 0 & 1
\end{bmatrix}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
ENU 
\begin_inset Formula $\rightarrow$
\end_inset

 ECEF:
\begin_inset Formula 
\begin{equation}
\begin{bmatrix}x\\
y\\
z
\end{bmatrix}=\mathbf{R}_{3}\left[-\left(\frac{\pi}{2}+\lambda\right)\right]\mathbf{R}_{1}\left[-\left(\frac{\pi}{2}-\phi\right)\right]\begin{bmatrix}E\\
N\\
U
\end{bmatrix}=\begin{bmatrix}-\sin\lambda & -\cos\lambda\sin\phi & \cos\lambda\cos\phi\\
\cos\lambda & -\sin\lambda\sin\phi & \sin\lambda\cos\phi\\
0 & \cos\phi & \sin\phi
\end{bmatrix}\begin{bmatrix}E\\
N\\
U
\end{bmatrix}
\end{equation}

\end_inset

ECEF 
\begin_inset Formula $\rightarrow$
\end_inset

 ENU:
\begin_inset Formula 
\begin{equation}
\begin{bmatrix}E\\
N\\
U
\end{bmatrix}=\mathbf{R}_{1}\left[\frac{\pi}{2}-\phi\right]\mathbf{R}_{3}\left[\frac{\pi}{2}+\lambda\right]\begin{bmatrix}x\\
y\\
z
\end{bmatrix}=\begin{bmatrix}-\sin\lambda & \cos\lambda & 0\\
-\cos\lambda\sin\phi & -\sin\lambda\sin\phi & \cos\phi\\
\cos\lambda\cos\phi & \sin\lambda\cos\phi & \sin\phi
\end{bmatrix}\begin{bmatrix}x\\
y\\
z
\end{bmatrix}
\end{equation}

\end_inset

Purdue Geodetic: 
\begin_inset Formula $\lambda_{0},\phi_{0},h_{0}$
\end_inset


\begin_inset Newline newline
\end_inset

Purdue ECEF: 
\begin_inset Formula $\begin{bmatrix}x_{0}\\
y_{0}\\
z_{0}
\end{bmatrix}=\begin{bmatrix}\left(R_{E}+h_{0}\right)\cos\left(\phi\right)\cos\left(\lambda\right)\\
\left(R_{E}+h_{0}\right)\cos\left(\phi\right)\sin\left(\lambda\right)\\
\left(R_{E}+h_{0}\right)\sin\left(\phi\right)
\end{bmatrix}$
\end_inset


\begin_inset Newline newline
\end_inset

