\begin_inset Formula 
\[
F\left(r\right)=-\frac{GMm}{r^{2}}+\frac{L^{2}}{\mu r^{3}}-\frac{3G\left(M+m\right)L^{2}}{c^{2}\mu r^{4}}
\]

\end_inset


\begin_inset Formula 
\[
m=\frac{M\mu}{M-\mu}
\]

\end_inset


\begin_inset Formula 
\[
F\left(r\right)=-\frac{GM^{2}\mu}{\left(M-\mu\right)r^{2}}+\frac{L^{2}}{\mu r^{3}}-\frac{3GM^{2}L^{2}}{c^{2}\mu\left(M-\mu\right)r^{4}}
\]

\end_inset

$$
\begin_inset Formula 
\[
F\left(r\right)=-\frac{GMm}{r^{2}}+\frac{L^{2}}{\mu r^{3}}-\frac{3G\left(M+m\right)L^{2}}{c^{2}\mu r^{4}}
\]

\end_inset


\begin_inset Formula 
\[
m_{1}\ddot{x}=-\frac{Gm_{0}m_{1}}{r^{2}}+\frac{\left(m_{0}+m_{1}\right)L^{2}}{m_{0}m_{1}r^{3}}-\frac{3G\left(m_{0}+m_{1}\right)^{2}L^{2}}{c^{2}m_{0}m_{1}r^{4}}
\]

\end_inset


\begin_inset Formula 
\[
\ddot{x}=-\frac{Gm_{0}}{r^{2}}+\frac{\left(m_{0}+m_{1}\right)L^{2}}{m_{0}m_{1}^{2}r^{3}}-\frac{3G\left(m_{0}+m_{1}\right)^{2}L^{2}}{c^{2}m_{0}m_{1}^{2}r^{4}}
\]

\end_inset


\begin_inset Formula 
\[
\frac{r_{c}}{t_{c}^{2}}\ddot{\rho}=-\frac{Gm_{0}}{r_{c}^{2}\rho^{2}}+\frac{\left(m_{0}+m_{1}\right)L^{2}}{m_{0}m_{1}^{2}r_{c}^{3}\rho^{3}}-\frac{3G\left(m_{0}+m_{1}\right)^{2}L^{2}}{c^{2}m_{0}m_{1}^{2}r_{c}^{4}\rho^{4}}
\]

\end_inset


\begin_inset Formula 
\[
\frac{r_{c}}{t_{c}^{2}}\ddot{\rho}=-\frac{Gm_{c}\mu_{0}}{r_{c}^{2}\rho^{2}}+\frac{m_{c}\left(\mu_{0}+\mu_{1}\right)L^{2}}{m_{c}^{3}\mu_{0}\mu_{1}^{2}r_{c}^{3}\rho^{3}}-\frac{3Gm_{c}^{2}\left(\mu_{0}+\mu_{1}\right)^{2}L^{2}}{c^{2}m_{c}^{3}\mu_{0}\mu_{1}^{2}r_{c}^{4}\rho^{4}}
\]

\end_inset


\begin_inset Formula 
\[
\ddot{\rho}=-\frac{m_{c}t_{c}^{2}}{r_{c}^{3}}G\mu_{0}\frac{1}{\rho^{2}}+\frac{m_{c}t_{c}^{2}}{m_{c}^{3}r_{c}^{4}}L^{2}\frac{\left(\mu_{0}+\mu_{1}\right)}{\mu_{0}\mu_{1}^{2}}\frac{1}{\rho^{3}}-\frac{m_{c}^{2}t_{c}^{2}}{m_{c}^{3}r_{c}^{5}}\frac{3GL^{2}}{c^{2}}\frac{\left(\mu_{0}+\mu_{1}\right)^{2}}{\mu_{0}\mu_{1}^{2}}\frac{1}{\rho^{4}}
\]

\end_inset


\begin_inset Formula 
\[
\ddot{\rho}=-\frac{m_{c}t_{c}^{2}}{r_{c}^{3}}G\mu_{0}\frac{1}{\rho^{2}}+\frac{t_{c}^{2}}{m_{c}^{2}r_{c}^{4}}L^{2}\frac{\left(\mu_{0}+\mu_{1}\right)}{\mu_{0}\mu_{1}^{2}}\frac{1}{\rho^{3}}-\frac{t_{c}^{2}}{m_{c}r_{c}^{5}}\frac{3GL^{2}}{c^{2}}\frac{\left(\mu_{0}+\mu_{1}\right)^{2}}{\mu_{0}\mu_{1}^{2}}\frac{1}{\rho^{4}}
\]

\end_inset


