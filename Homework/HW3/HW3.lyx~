\begin_inset Formula 
\[
\frac{r_{c}}{t_{c}^{2}}\ddot{\vec{\chi}}=\begin{pmatrix}-\frac{m_{c}}{r_{c}^{2}}G\left(\frac{\mu_{1}\left(\vec{\rho}_{0}-\vec{\rho}_{1}\right)}{|\vec{\rho}_{0}-\vec{\rho}_{1}|^{3}}+\frac{\mu_{2}\left(\vec{\rho}_{0}-\vec{\rho}_{2}\right)}{|\vec{\rho}_{0}-\vec{\rho}_{2}|^{3}}\right)\\
-\frac{m_{c}}{r_{c}^{2}}G\left(\frac{\mu_{0}\left(\vec{\rho}_{1}-\vec{\rho}_{0}\right)}{|\vec{\rho}_{1}-\vec{\rho}_{0}|^{3}}+\frac{\mu_{2}\left(\vec{\rho}_{1}-\vec{\rho}_{2}\right)}{|\vec{\rho}_{1}-\vec{\rho}_{2}|^{3}}\right)\\
-\frac{m_{c}}{r_{c}^{2}}G\left(\frac{\mu_{0}\left(\vec{\rho}_{2}-\vec{\rho}_{0}\right)}{|\vec{\rho}_{2}-\vec{\rho}_{0}|^{3}}+\frac{\mu_{1}\left(\vec{\rho}_{2}-\vec{\rho}_{1}\right)}{|\vec{\rho}_{2}-\vec{\rho}_{1}|^{3}}\right)
\end{pmatrix}
