\begin_body

\begin_layout Standard
\begin_inset Formula 
\[
\vec{T}=\frac{3GM}{r^{3}}\left(C-A\right)\sin\delta\cos\delta\begin{pmatrix}\sin\alpha\\
-\cos\alpha\\
0
\end{pmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\vec{T}=\frac{3GM}{r^{3}}\left(B_{\text{axis of rotation}}-A_{\text{equatorial diameter}}\right)\sin\delta\cos\delta\begin{pmatrix}\sin\alpha\\
-\cos\alpha\\
0
\end{pmatrix}
\]

\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula 
\[
\vec{T}_{\text{on Hyperion}}=\frac{3GM_{\text{Saturn}}}{r^{3}}\left(C_{\text{Hyperion,axis rot}}-A_{\text{Hyperion, equatorial dia}}\right)\sin\delta\cos\delta\begin{pmatrix}\sin\alpha\\
-\cos\alpha\\
0
\end{pmatrix}
\]

\end_inset


\begin_inset Formula 
\[
\vec{T}_{\text{on Hyperion}}=\frac{3GM_{\text{Saturn}}}{r^{3}}\left(C_{\text{Hyperion,axis rot}}-A_{\text{Hyperion, equatorial dia}}\right)\sin\delta\cos\delta\begin{pmatrix}\sin\alpha\\
-\cos\alpha\\
0
\end{pmatrix}
\]

\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula 
\[
A\frac{d\omega_{a}}{dt}-\omega_{b}\omega_{c}(B-C)=-\frac{3}{r^{3}}\beta\gamma(B-C)
\]

\end_inset


\begin_inset Formula 
\[
B\frac{d\omega_{b}}{dt}-\omega_{c}\omega_{a}(C-A)=-\frac{3}{r^{3}}\gamma\alpha(C-A)
\]

\end_inset


\begin_inset Formula 
\[
C\frac{d\omega_{c}}{dt}-\omega_{a}\omega_{b}(A-B)=-\frac{3}{r^{3}}\alpha\beta(A-B)
\]

\end_inset


\begin_inset Newline newline
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\omega_{a}=\dot{\theta}\sin\phi\sin\psi+\dot{\phi}\cos\psi
\]

\end_inset


\begin_inset Formula 
\[
\omega_{b}=\dot{\theta}\sin\phi\cos\psi+\dot{\phi}\sin\psi
\]

\end_inset


\begin_inset Formula 
\[
\omega_{c}=\dot{\theta}\cos\phi+\dot{\psi}
\]

\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula 
\[
\dot{\omega}_{a}=\ddot{\theta}\sin\phi\sin\psi+\dot{\theta}\left(\dot{\phi}\cos\phi\sin\psi+\dot{\psi}\sin\phi\cos\psi\right)+\ddot{\phi}\cos\psi-\dot{\phi}\dot{\psi}\sin\psi
\]

\end_inset


\begin_inset Formula 
\[
\dot{\omega}_{b}=\ddot{\theta}\sin\phi\cos\psi+\dot{\theta}\left(\dot{\phi}\cos\phi\cos\psi-\dot{\psi}\sin\phi\sin\psi\right)+\ddot{\phi}\sin\psi+\dot{\phi}\dot{\psi}\cos\psi
\]

\end_inset


\begin_inset Formula 
\[
\dot{\omega}_{c}=\ddot{\theta}\cos\phi-\dot{\theta}\dot{\phi}\sin\phi+\ddot{\psi}
\]

\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula 
\[
\alpha=\cos\theta\cos\psi-\sin\theta\cos\phi\sin\psi
\]

\end_inset


\begin_inset Formula 
\[
\beta=-\cos\theta\sin\psi-\sin\theta\cos\phi\cos\psi
\]

\end_inset


\begin_inset Formula 
\[
\gamma=\sin\theta\sin\phi
\]

\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula 
\[
A\left(\ddot{\theta}\sin\phi\sin\psi+\dot{\theta}\left(\dot{\phi}\cos\phi\sin\psi+\dot{\psi}\sin\phi\cos\psi\right)+\ddot{\phi}\cos\psi-\dot{\phi}\dot{\psi}\sin\psi\right)-\left(\dot{\theta}\sin\phi\cos\psi+\dot{\phi}\sin\psi\right)\left(\dot{\theta}\cos\phi+\dot{\psi}\right)(B-C)=-\frac{3}{r^{3}}\left(-\cos\theta\sin\psi-\sin\theta\cos\phi\cos\psi\right)\left(\sin\theta\sin\phi\right)(B-C)
\]

\end_inset


\begin_inset Formula 
\[
B\frac{d\omega_{b}}{dt}-\omega_{c}\omega_{a}(C-A)=-\frac{3}{r^{3}}\gamma\alpha(C-A)
\]

\end_inset


\begin_inset Formula 
\[
C\frac{d\omega_{c}}{dt}-\omega_{a}\omega_{b}(A-B)=-\frac{3}{r^{3}}\alpha\beta(A-B)
\]

\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula 
\[
C\ddot{\theta}-\frac{3\left(B-A\right)}{2}\frac{GM}{r^{3}}\sin2\left(\theta-f\right)=0\Rightarrow\ddot{\theta}+\frac{\omega_{0}^{2}}{2r^{3}}\sin2\left(\theta-f\right)=0
\]

\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula 
\[
A\frac{d\omega_{a}}{dt}-\omega_{b}\omega_{c}(B-C)=-\frac{3}{r^{3}}\left(-\cos\theta\sin\psi-\sin\theta\cos\phi\cos\psi\right)\sin\theta\sin\phi(B-C)
\]

\end_inset


\begin_inset Formula 
\[
B\frac{d\omega_{b}}{dt}-\omega_{c}\omega_{a}(C-A)=-\frac{3}{r^{3}}\sin\theta\sin\phi\left(\cos\theta\cos\psi-\sin\theta\cos\phi\sin\psi\right)(C-A)
\]

\end_inset


\begin_inset Formula 
\[
C\frac{d\omega_{c}}{dt}-\omega_{a}\omega_{b}(A-B)=-\frac{3}{r^{3}}\left(\cos\theta\cos\psi-\sin\theta\cos\phi\sin\psi\right)\left(-\cos\theta\sin\psi-\sin\theta\cos\phi\cos\psi\right)(A-B)
\]

\end_inset


\end_layout

